<interface>
  <!-- interface-name editionpanel.ui -->
  <requires lib="gtk" version="4.0"/>
  <template class="MDotEditionPanel" parent="GtkBox">
    <child>
      <object class="GtkPaned">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkStackSwitcher">
            <property name="stack">
              <object class="GtkStack">
                <child>
                  <object class="GtkStackPage">
                    <property name="child">
                      <object class="GtkLabel">
                        <property name="label">Empty Page</property>
                      </object>
                    </property>
                    <property name="name">mcd-tools</property>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
